{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "@wemake-services/typescript/recommended",
      "@wemake-services/javascript"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "unicorn/filename-case": "off",
      "unicorn/no-null": "off",
      "id-length": "off",
      "no-shadow": "off",
      "no-confusing-arrow": "off",
      "max-len": ["error", { "code": 125 }]
    }
  }
  